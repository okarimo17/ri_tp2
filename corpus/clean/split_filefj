ino","alphonse","juin","rené","huyghe","jean","paulhan","pierre-henri","simon","jean","rostand","jean","guéhenno","10","jean","guitton","11","maurice","garçon","12","jules","romains","13","wladimir","14","jean","mistler","15","andré","chamson","16","antoine","lévis-mirepoix","17","jean","delay","18","andré","françois-poncet","19","rené","clair","20","thierry","maulnier","21","marcel","achard","22","françois","mauriac","23","gilson","24","pasteur","vallery-radot","25","marcel","pagnol","26","andré","maurois","27","joseph","kessel","28","henri","troyat","29","henry","montherlant","30","fauteuil","vacant","31","jacques","rueff","32","henri","massis","33","marcel","brion","34","maurice","genevoix","35","louis","leprince-ringuet","36","pierre","gaxotte","37","eugène","tisserant","38","louis","armand","39","jacques","lacretelle","40","jacques","chastenet","date","1élection","1933","françois","mauriac","1936","jacques","lacretelle","1938","andré","maurois","1944","louis","broglie","1944","louis","pasteur","vallery-radot","1946","maurice","garçon","1946","marcel","pagnol","1946","jules","romains","1946","gilson","1946","maurice","genevoix","1952","andré","françois-poncet","1952","alphonse","juin","1953","pierre","gaxotte","1953","antoine","lévis-mirepoix","1955","jérôme","carcopino","1956","wladimir","1956","andré","chamson","1956","jacques","chastenet","1959","jean","rostand","1959","henri","troyat","1959","marcel","achard","1959","jean","delay","1960","henry","montherlant","1960","henri","massis","1960","rené","huyghe","1960","rené","clair","1961","jean","guitton","1961","eugène","tisserant","1962","jean","guéhenno","1962","marc","boegner","1962","joseph","kessel","1963","jean","paulhan","1963","louis","armand","1964","thierry","maulnier","1964","marcel","brion","1964","jacques","rueff","1966","louis","leprince-ringuet","1966","jean","mistler","1966","pierre-henri","simon","xxx","mcomposition","démie","française","jour","mort","numéro","fauteuil","louis","broglie","rené",